<div class="contents row">
  <div class="container">
    <%= form_with(model: @tweet, local: true) do |f| %>
      <h3>
        編集する
      </h3>
      <%= f.text_field :image, placeholder: "画像URL" %>
      <%= f.text_area :text, placeholder: "文字入力" , rows: "10" %>
      <%= f.submit "更新" %>
    <% end %>
  </div>
</div>