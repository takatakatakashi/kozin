<div class="contents row">
  <div class="container">
    <%= form_with(model: @tweet, local: true) do |form| %>
      <h3>
        編集する
      </h3>
      <%= form.text_field :image, placeholder: "画像URL" %>
      <%= form.text_area :text, placeholder: "文字入力" , rows: "10" %>
      <%= form.submit "更新" %>
    <% end %>
  </div>
</div>